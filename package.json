{
  "name": "tailadmin-vue-pro-2.0.1",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "nodemon server.js",
    "dev:backend:alt": "node start-backend.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:full:alt": "concurrently \"npm run dev\" \"npm run dev:backend:alt\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy": "npm run build && gh-pages -d dist",
    "build:gh-pages": "NODE_ENV=production npm run build",
    "start:backend": "node server.js",
    "init:database": "node src/backend/scripts/initDatabase.js",
    "test:database": "node src/backend/scripts/testConnection.js",
    "debug:api": "node src/backend/scripts/debugApi.js",
    "start:full": "concurrently \"npm run preview\" \"npm run start:backend\"",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "seed:orders": "node src/backend/scripts/seedOrders.js",
    "seed:database": "node src/backend/scripts/seedDatabase.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@fullcalendar/vue3": "^6.1.15",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "apexcharts": "^4.4.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.2",
    "flatpickr": "^4.6.13",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jsvectormap": "^1.6.0",
    "lucide-vue-next": "^0.474.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "sequelize": "^6.37.7",
    "swiper": "^11.2.1",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.3",
    "vue-flatpickr-component": "^11.0.5",
    "vue-i18n": "^12.0.0-alpha.3",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0",
    "vuedraggable": "^4.1.0",
    "vuevectormap": "^2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^30.2.0",
    "jiti": "^2.4.2",
    "nodemon": "^3.1.7",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.4",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.6.3",
    "vite": "^6.0.3",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-tsc": "^2.1.10"
  }
}
